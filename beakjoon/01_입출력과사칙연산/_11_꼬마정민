package _01_입출력과사칙연산;

// 입력			출력
// 77 77 7777	7931

import java.util.Scanner;

public class _11_꼬마정민 {
	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	
	// int형보다 큰 값을 출력할 수도 있으므로 long 형(8byte)을 사용하여 출력함
	long A = sc.nextLong();
	long B = sc.nextLong();
	long C = sc.nextLong();
	
	System.out.println(A + B + C);
    }
}
